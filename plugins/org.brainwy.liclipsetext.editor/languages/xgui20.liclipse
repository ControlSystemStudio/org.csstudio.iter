inherit: BaseDefinitions # Signal that we want to start from base.liclipse

scope_to_color_name: {
 #All types of strings map to the string color
 doubleQuotedMultiLineString: string,
 singleQuotedMultiLineString: string,
 doubleQuotedString: string,
 singleQuotedString: string,
}

scope_definition_rules:
  #Comments (inherited from base.liclipse)
  - 'HashCommentRule'

  #Multi Line Strings (inherited from base.liclipse)
  #It's important that the multi-line comes before the single-line (otherwise the single line would always match first
  #and would never allow the multi-line to work).
  - 'SingleQuotedTriMultiLineRule'
  - 'DoubleQuotedTriMultiLineRule'

  # Single Line Strings (inherited from base.liclipse)
  - 'SingleQuotedSingleLineRule'
  - 'DoubleQuotedSingleLineRule'

scope:
  singleLineComment:
    javadocTag: [TODO] #Any TODO inside a comment should have a different color.

  default:
    sub_rules: [
      {type: CompositeRule, sub_rules: [
        { type: MatchLineStartRule, scope: default},
        { type: ZeroOrMoreSpacesRule, scope: default},
        { type: AnyWordRule, scope: class}, #Anything before '=' (i.e.: editor = Widget) -- but does not match when there's a '.' in the start.
        { type: OneOrMoreSpacesRule, scope: default},
        { type: SequenceRule, scope: operator, sequence: '='}
        ]
      },
      'NumberRule',
    ]

    keyword: ['BoolColumn', 'Boolean', 'CheckGroup', 'ColorButton', 'ColorColumn', 'Command', 'DateTime', 'DateTimeColumn', 'Dialog', 'Dict', 'DynamicTable', 'Editors', 'Filename', 'FixedArray', 'Float', 'FloatColumn', 'FloatQuantityColumn', 'FontButton', 'FractionColumn', 'FractionScalar', 'Group', 'HorizontalContainer', 'Image', 'Import', 'IntColumn', 'Integer', 'MultiDataChoice', 'MultiDataChoiceItem', 'Page', 'Pages', 'Scalar', 'ScalarColumn', 'Select1', 'Select1Column', 'SelectN', 'Splitter', 'SplitterPage', 'StringColumn', 'SubjectTable', 'Tab', 'Table', 'TableColumn', 'TableToolBar', 'TableView', 'Tabs', 'Text', 'ToolBar', 'ToolButton', 'Tree', 'TreeView', 'TreeWidget', 'TupleTable', 'Widget']

    method: ['add_spacer', 'align', 'alignment', 'alternate_row_colors', 'auto_default', 'auto_fit', 'auto_raise', 'background_color_expr', 'background_color_refresh', 'base_class', 'buttons', 'calendar', 'caption', 'caption_cmd', 'caption_color_expr', 'caption_position', 'caption_refresh', 'captions_cmd', 'captions_refresh', 'category', 'check_cmd', 'checkable', 'class_name', 'cmd', 'columns_hidden_cmd', 'columns_hidden_refresh', 'context_menu_callback', 'controller', 'create_caption', 'create_page_cmd', 'data', 'data_refresh', 'decimals', 'default_unit', 'dimension', 'double_click_cmd', 'double_clicked_cmd', 'editing_status_cmd', 'enable_expr', 'enable_refresh', 'entity_orientation', 'expanded_expr', 'expanded_refresh', 'file_type', 'file_types_cmd', 'filename', 'float_format', 'format_expr', 'frame_shape', 'height', 'hidden_subject_indexes_cmd', 'hidden_subject_indexes_refresh', 'horizontal_header', 'horizontal_size_policy', 'horizontal_size_type', 'horizontal_stretchable', 'horstreth', 'img', 'indent', 'initial_dir_cmd', 'initial_filter_cmd', 'io_mode', 'is_main', 'item_captions', 'keep_state', 'key', 'layout_margin', 'layout_spacing', 'left_margin', 'margin_indent', 'margin_width', 'max_value_expr', 'min_max_refresh', 'min_value_expr', 'normalized', 'obtain_data_cmd', 'obtain_editor_id_cmd', 'one_based_index_expr', 'only_fraction_part', 'orientation', 'read_only_expr', 'read_only_refresh', 'refresh_on', 'resize_columns_automatically', 'root_is_decorated', 'searchable', 'selected_cell_cmd', 'selection_cmd', 'selection_mode', 'should_trigger_modify_while_import', 'should_validate_by_category', 'show_buttons', 'show_unit', 'show_vertical_header', 'show_whats_this', 'single_step', 'size_adjust_policy', 'sorting_enabled', 'spacer_policy', 'spacer_resize_policy', 'stretchable', 'style', 'sub_fields', 'text_alignment', 'tool_tip_expr', 'tool_tip_refresh', 'tooltip_callback', 'top_layout_margin', 'top_layout_spacing', 'unit_row_enabled', 'unit_style', 'unit_width', 'use_apply_on_request', 'use_child_headers', 'validation_callback', 'validation_expr', 'value', 'value_type', 'values', 'values_cmd', 'values_refresh', 'values_sorted', 'vertical_header', 'vertical_size_policy', 'vertical_size_type', 'visible_count', 'visible_expr', 'visible_refresh', 'whats_this', 'width']

    number: ['False', 'True']

    bracket: ['(', ')', '[', ']', '{', '}']

    operator: [<, '>', '=', +, '-', /, '*', '!', '&', '|', '%', '~', ^, ',']


file_extensions: [xgui2, xgui2_, xinc]
filename: []
name: Xgui20

outline: #Note that we just specify 'flat' items here, the indent is later used to specify parent/children scopes.
  - {type: Scope, scope: [default, class], define: class} #Wherever we have a class inside the default scope we'll show a class icon in the outline.

indent: {
  type: spaces, #Our example language uses spaces for indenting
  outline_scopes: [class], #We have to say which outline entries actually create a new scope (so, indent and outline work toghether to specify the tree).
  spaces_for_tabs: true, #Python folks recommend it.
  tab_width: 4, #Note: optional (default is based on preferences for text editors in the eclipse preferences)
}

# Specify that the default comment action (Ctrl+/) deals with creating '#' at the start of the line
comment: {type: singleLine, sequence: '#'}

auto_edit:
  - {auto_close_scopes: [doubleQuotedString, singleQuotedString, singleQuotedMultiLineString, doubleQuotedMultiLineString], scope: default} #To work, the scopes must be: SingleLineRule(sequence used for start/end) or MultiLineRule.
  - {auto_close_parens: ['(', '[', '{'], scope: default} #Will close the parens if it's not properly balanced
  - {auto_skip_parens: [')', ']', '}'], scope: default} #Will skip the close parens if it's properly balanced


# Ok, basic structure in place, now, just config code-completion / templates for the language.
code_completion: {
  use_only_templates: true,
}

