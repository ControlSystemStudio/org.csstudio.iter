- name: 'new_widget'
  description: "Widget"
  pattern: |-
    editor = Widget
        .class_name = '${MyXGuiWidget}'
        .caption = ''
        .controller = '${ControllerClass}'
        .spacer_resize_policy = 'Minimum'

