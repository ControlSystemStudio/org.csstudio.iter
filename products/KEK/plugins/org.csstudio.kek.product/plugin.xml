<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.csstudio.startup.application.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.csstudio.kek.product.application"
            name="KEK/J-PARC CSS">
         <property
               name="appName"
               value="KEK/J-PARC CSS">
         </property>
         <property
               name="aboutImage"
               value="platform:/plugin/org.csstudio.utility.product/icons/aboutSide.png">
         </property>
         <property
               name="aboutText"
               value="Control System Studio (CSS) is a combined&#x0A;effort of several parties, including&#x0A;- DESY (Hamburg, Germany)&#x0A;- SNS (Oak Ridge, TN)&#x0A;- BNL (Brookhaven, NY)&#x0A;- KEK/J-PARC&#x0A;&#x0A;It provides a collection of control system&#x0A;tools in a common environment, based&#x0A;on Eclipse.&#x0A;&#x0A;http://sourceforge.net/apps/trac/cs-studio/wiki">
         </property>
         <property
               name="windowImages"
               value="platform:/plugin/org.csstudio.utility.product/icons/css16.png,platform:/plugin/org.csstudio.utility.product/icons/css32.png,platform:/plugin/org.csstudio.utility.product/icons/css48.png,platform:/plugin/org.csstudio.utility.product/icons/css64.png,platform:/plugin/org.csstudio.utility.product/icons/css128.png">
         </property>
         <property
               name="startupForegroundColor"
               value="FFFFFF">
         </property>
         <property
               name="startupMessageRect"
               value="20,200,340,20">
         </property>
         <property
               name="startupProgressRect"
               value="20,220,340,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
   <extension
         point="org.csstudio.startup.module">
      <startupParameters
            class="org.csstudio.utility.product.StartupParameters">
      </startupParameters>
      <workbench
            class="org.csstudio.utility.product.Workbench">
      </workbench>
   </extension>

</plugin>
