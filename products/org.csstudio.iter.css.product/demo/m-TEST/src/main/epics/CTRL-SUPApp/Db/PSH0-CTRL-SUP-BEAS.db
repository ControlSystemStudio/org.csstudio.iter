record (bo,"CTRL-SUP-BEAS:PUMP")
{
	field(DESC, "Pump ON/OFF")
	field(DOL, "0")
	field(IVOA, "Set output to IVOV")
	field(IVOV, "0")
	field(ONAM, "ON")
	field(PINI, "YES")
	field(SCAN, "1 second")
	field(ZNAM, "OFF")
}
record (bi,"CTRL-SUP-BEAS:HIGH-SWITCH")
{
	field(DESC, "Tank High level")
	field(INP, "0")
	field(ONAM, "Wet")
	field(OSV, "MAJOR")
	field(PINI, "YES")
	field(SCAN, "1 second")
	field(ZNAM, "Dry")
	field(ZSV, "NO_ALARM")
}
record (calcout,"CTRL-SUP-BEAS:HIGH-CHECK")
{
	field(CALC, "A")
	field(DESC, "Force ON pump?")
	field(DOPT, "Use OCAL")
	field(INPA, "CTRL-SUP-BEAS:HIGH-SWITCH CP MS")
	field(OCAL, "1")
	field(OOPT, "Transition To Non-zero")
	field(OUT, "CTRL-SUP-BEAS:PUMP PP")
}
record (bi,"CTRL-SUP-BEAS:LOW-SWITCH")
{
	field(DESC, "Tank Low level")
	field(INP, "0")
	field(ONAM, "Wet")
	field(OSV, "NO_ALARM")
	field(PINI, "YES")
	field(SCAN, "1 second")
	field(ZNAM, "Dry")
	field(ZSV, "MAJOR")
}
record (calcout,"CTRL-SUP-BEAS:LOW-CHECK")
{
	field(CALC, "A")
	field(DESC, "Force OFF pump?")
	field(DOPT, "Use OCAL")
	field(INPA, "CTRL-SUP-BEAS:LOW-SWITCH CP MS")
	field(OCAL, "0")
	field(OOPT, "Transition To Zero")
	field(OUT, "CTRL-SUP-BEAS:PUMP PP")
}
record (bi,"CTRL-SUP-BEAS:MID-SWITCH")
{
	field(DESC, "Tank Mid level")
	field(INP, "0")
	field(ONAM, "Wet")
	field(PINI, "YES")
	field(SCAN, "1 second")
	field(ZNAM, "Dry")
}
record (calcout,"CTRL-SUP-BEAS:MID-CHECK")
{
	field(CALC, "A")
	field(DESC, "Start pump?")
	field(DOPT, "Use OCAL")
	field(INPA, "CTRL-SUP-BEAS:MID-SWITCH CP MS")
	field(OCAL, "1")
	field(OOPT, "Transition To Non-zero")
	field(OUT, "CTRL-SUP-BEAS:PUMP PP")
}
record (calcout,"CTRL-SUP-BEAS:TIMER-START")
{
	field(CALC, "!C && !D && (A == 1 || (B > 0 && B <=25))")
	field(DESC, "Start timer?")
	field(INPA, "CTRL-SUP-BEAS:MID-SWITCH CP MS")
	field(INPB, "CTRL-SUP-BEAS:MID-TIMER CP")
	field(INPC, "CTRL-SUP-BEAS:TANK-INTERLOCK CP")
	field(INPD, "CTRL-SUP-BEAS:TANK-MODE CP")
	field(OCAL, "0")
	field(OOPT, "Transition To Zero")
	field(OUT, "CTRL-SUP-BEAS:MID-TIMER PP")
}
record (calcout,"CTRL-SUP-BEAS:TIMER-CHECK")
{
	field(CALC, "A")
	field(DESC, "Stop pump?")
	field(DOPT, "Use OCAL")
	field(INPA, "CTRL-SUP-BEAS:TIMER-START CP MS")
	field(OCAL, "0")
	field(OOPT, "Transition To Zero")
	field(OUT, "CTRL-SUP-BEAS:PUMP PP")
}
record (calc,"CTRL-SUP-BEAS:MID-TIMER")
{
	field(CALC, "A <= 25 ? A+1:0")
	field(DESC, "Timer of 25 seconds")
	field(DISV, "0")
	field(INPA, "CTRL-SUP-BEAS:MID-TIMER")
	field(SCAN, "1 second")
	field(SDIS, "CTRL-SUP-BEAS:TIMER-START")
	field(VAL, "0")
}
record (bo,"CTRL-SUP-BEAS:MID-SWITCH-FAIL")
{
	field(DESC, "Failure of MID switch")
	field(DOL, "0")
	field(ONAM, "ON")
	field(OSV, "MAJOR")
	field(PINI, "YES")
	field(SCAN, "1 second")
	field(ZNAM, "OFF")
	field(ZSV, "NO_ALARM")
}
record (calc,"CTRL-SUP-BEAS:TANK-LEVEL")
{
	field(ADEL, "10")
	field(CALC, "(!B && !C)?A+2*RNDM:A-2*RNDM")
	field(DESC, "Tank level with ADEL")
	field(DISV, "1")
	field(EGU, "m3")
	field(HHSV, "MAJOR")
	field(HIGH, "80")
	field(HIHI, "90")
	field(HOPR, "100")
	field(HSV, "MINOR")
	field(INPA, "CTRL-SUP-BEAS:TANK-LEVEL")
	field(INPB, "CTRL-SUP-BEAS:TANK-MID")
	field(INPC, "CTRL-SUP-BEAS:TIMER-START")
	field(LLSV, "MAJOR")
	field(LOLO, "10")
	field(LOPR, "0")
	field(LOW, "20")
	field(LSV, "MINOR")
	field(PREC, "2")
	field(SCAN, "2 second")
	field(SDIS, "CTRL-SUP-BEAS:TANK-INTERLOCK")
	field(VAL, "35")
}
record (calcout,"CTRL-SUP-BEAS:TANK-HIGH")
{
	field(CALC, "A > B")
	field(DESC, "High Level?")
	field(INPA, "CTRL-SUP-BEAS:TANK-LEVEL CP MS")
	field(INPB, "CTRL-SUP-BEAS:TANK-LEVEL.HIGH")
	field(OOPT, "On Change")
	field(OUT, "CTRL-SUP-BEAS:HIGH-SWITCH PP")
}
record (calcout,"CTRL-SUP-BEAS:TANK-LOW")
{
	field(CALC, "!(A < B)")
	field(DESC, "Low Level?")
	field(INPA, "CTRL-SUP-BEAS:TANK-LEVEL CP MS")
	field(INPB, "CTRL-SUP-BEAS:TANK-LEVEL.LOW")
	field(OOPT, "On Change")
	field(OUT, "CTRL-SUP-BEAS:LOW-SWITCH PP")
}
record (calcout,"CTRL-SUP-BEAS:TANK-MID")
{
	field(CALC, "A > B/2 - 5")
	field(DESC, "Mid Level?")
	field(DISV, "1")
	field(INPA, "CTRL-SUP-BEAS:TANK-LEVEL CP MS")
	field(INPB, "CTRL-SUP-BEAS:TANK-LEVEL.HOPR")
	field(OOPT, "On Change")
	field(OUT, "CTRL-SUP-BEAS:MID-SWITCH PP")
	field(SDIS, "CTRL-SUP-BEAS:MID-SWITCH-FAIL")
}
record (calcout,"CTRL-SUP-BEAS:TANK-INTERLOCK")
{
	field(CALC, "(A >= B-5 || A <= C+5) && !D")
	field(DESC, "Tank Interlock")
	field(INPA, "CTRL-SUP-BEAS:TANK-LEVEL CP MS")
	field(INPB, "CTRL-SUP-BEAS:TANK-LEVEL.HOPR")
	field(INPC, "CTRL-SUP-BEAS:TANK-LEVEL.LOPR")
	field(INPD, "CTRL-SUP-BEAS:TANK-MODE CP")
	field(IVOA, "Set output to IVOV")
	field(IVOV, "1")
}
record (bo,"CTRL-SUP-BEAS:TANK-MODE")
{
	field(DESC, "Allow to reset the process")
	field(HIGH, "15.0")
	field(ONAM, "Override")
	field(PINI, "YES")
	field(VAL, "0")
	field(ZNAM, "Operational")
}
record (stringin,"CTRL-SUP-BEAS:IOC-TIME")
{
	field(DESC, "IOC Current Date and Time")
	field(DTYP, "Soft Timestamp")
	field(INP, "@%d/%m/%Y %H:%M:%S")
	field(PINI, "YES")
	field(SCAN, "1 second")
}
