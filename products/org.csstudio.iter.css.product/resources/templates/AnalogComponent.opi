                <scriptText><![CDATA[from org.csstudio.opibuilder.scriptUtil import PVUtil

from java.lang import Thread, Runnable

class onGoingTask(Runnable):
    def run(self): 

        request = PVUtil.getDouble(pvs[0])

        position = PVUtil.getDouble(pvs[1])

        result = pvs[1]

        i=0
        if request < position:
          i = -1
        elif request > position:
          i = 1
        
        while (position != request):
           position = position + i
           result.setValue(position)
           Thread.sleep(1000)

thread =Thread(onGoingTask());
thread.start()
]]></scriptText>
