                <scriptText><![CDATA[from org.csstudio.opibuilder.scriptUtil import PVUtil
from org.eclipse.jface.dialogs import MessageDialog

request   = PVUtil.getDouble(pvs[0])
limit_on  = PVUtil.getDouble(pvs[1])
limit_off = PVUtil.getDouble(pvs[2])
max_range = PVUtil.getDouble(pvs[3])
min_range = PVUtil.getDouble(pvs[4])

MessageDialog.openInformation(
	None, "Input Limits", "Min " + str(min_range) + " Max " + str(max_range))


# Check out of range input
if request > max_range:
	MessageDialog.openError(
		None, "Input Validation Script", "Position request of " + str(request) + " is greater than Max Range of " + str(max_range))

elif request == max_range:
	MessageDialog.openWarning(
		None, "Input Validation Script", "Position request of " + str(request) + " is equal to Max Range of " + str(max_range))

elif request == min_range:
	MessageDialog.openWarning(
		None, "Input Validation Script", "Position request of " + str(request) + " is equal to Min Range of " + str(min_range))

elif request < min_range:
	MessageDialog.openError(
		None, "Input Validation Script", "Position request of " + str(request) + " is smaller than Min Range of " + str(min_range))

elif request >= limit_on:
	MessageDialog.openInformation(
		None, "Input Validation Script", "Position request of " + str(request) + " is greater or equal to the Limit ON of " + str(limit_on))

elif request <= limit_off:
	MessageDialog.openInformation(
		None, "Input Validation Script", "Position request of " + str(request) + " is smaller or equal to the Limit OFF of " + str(limit_off))
]]></scriptText>
